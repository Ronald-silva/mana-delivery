{
    "version": 2,
    "builds": [
      {
        "src": "js/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "css/**",
        "use": "@vercel/static"
      },
      {
        "src": "js/**",
        "use": "@vercel/static"
      },
      {
        "src": "img/**",
        "use": "@vercel/static"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/js/(.*)",
        "dest": "/js/$1",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/img/(.*)",
        "dest": "/img/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "js/server.js"
      },
      {
        "src": "/admin",
        "dest": "/admin.html"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }