{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://sanduiche-do-chefe.vercel.app/$1"
      },
      "continue": false,
      "has": [
        {
          "type": "host",
          "value": "(?!sanduiche-do-chefe.vercel.app).*"
        }
      ]
    },
    {
      "src": "/fonts/(.*)\\.(?:woff|woff2|eot|ttf|svg)$",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)\\.(?:js|css|png|jpg|jpeg|gif|ico|json|svg)$",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/sw.js",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      },
      "continue": true
    }
  ],
  "github": {
    "silent": false
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}
